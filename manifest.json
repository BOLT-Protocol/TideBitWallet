{
  "manifest_version": 3,
  "name": "TideBit Wallet",
  "version": "0.1.0",
  "description": "A HD Wallet for TideBit Chain, Ethereum and Bitcoin",

  "action": {
    "default_icon": {
      "16": "img/icon16.png",
      "48": "img/icon48.png",
      "128": "img/icon128.png"
    },
    "default_title": "TideBit Wallet",
    "default_popup": "/html/welcome.html"
  },

  "icons": {
    "16": "img/icon16.png",
    "48": "img/icon48.png",
    "128": "img/icon128.png"
  },

  "background": {
    "service_worker": "background.js"
  },

  "oauth2": {
    "client_id": "152626605441-fvfi1kicaqajs32veeee39p47lp9bf2f.apps.googleusercontent.com",
    "scopes":[
      "https://www.googleapis.com/auth/contacts.readonly"
    ]
  },

  "permissions": [
    "contextMenus",
    "storage",
    "activeTab",
    "identity"
  ],
  "content_scripts": [

  ]
}