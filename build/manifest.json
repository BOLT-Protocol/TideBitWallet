  "name": "TideBit Wallet",
  "version": "0.1.0",
  "action": {
    "default_icon": {
      "16": "image/icon16.png",
      "48": "image/icon48.png",
      "128": "image/icon128.png"
    },
    "default_title": "TideBit",
    "default_popup": "popup.html"
